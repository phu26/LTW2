          </div>
      <div style="background-image:url('/pic/xc.jpg')" class="card-deck mb-3 text-center">

      
        {{#each Host}}
        <div class="card mb-{TinyDes}}4 box-shadow">
    
         
            
          <div class="card-body">
            
            <h1 class="card-title pricing-card-title">$500 </h1>
            <ul class="list-unstyled mt-3 mb-4">
              <img  src="/pic/{{pic}}" width="150" height="150"  >
              <li>{{ProName}}</li>
              <li></li>
        
            </ul>
            <button class="btn btn-lg btn-block btn-outline-primary" type="button">Buy it</button>
             
          </div>
    
        </div>
             {{/each}}
      </div>
