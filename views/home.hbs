

   

    <main role="main">
      <div class="container-fluid mt-3"> 
          <div class="container-fluid mt-4"> 
           <div class="row">
              
             
               <div class="col-sm-9">
      <div class="card shadow">
        <h6><center>MAIN</center></h6>
      <section style="background-image:url('/pic/123.jpg')" class="jumbotron text-center"width="500" height="200" >
        <div  class="container"  >
          <div  class="carousel slide" id="myCarousel" data-ride="carousel">
        <ol class="carousel-indicators">
          <li data-target="#myCarousel" data-slide-to="0"></li>
          <li class="active" data-target="#myCarousel" data-slide-to="1"></li>
          <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>
        <div class="carousel-inner">
          <div class="carousel-item">
            <img class="first-slide" alt="First slide" src="/pic/1.jpg " width="400" height="250"  >
           
          </div>
          <div class="carousel-item active">
            <img class="second-slide" alt="Second slide" src="/pic/2.jpg" width="400" height="250">
           
          </div>
          <div class="carousel-item">
            <img class="third-slide" alt="Third slide" src="/pic/3.jpg" width="400" height="250">
         
          </div>
        </div>
     
        <a class="carousel-control-prev" role="button" href="#myCarousel" data-slide="prev">
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" role="button" href="#myCarousel" data-slide="next">
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </a>
      </div>
        </div>  
      </section>

     
      </div>
         </div>
                
             <div class="col-sm-3">
                   <div class="card shadow">
  <h5 class="card-header">
   <center > TIN TỨC </center>
  </h5>
 <div class="list-group">
            {{#each lcCategories}}
            <a href="/categories/{{CatID}}/products"
              class="list-group-item list-group-item-action {{#if active}} active {{/if}}">
              {{CatName}}
              {{#if num_of_products}}
              <span class="badge badge-danger float-right">{{num_of_products}}</span>
              {{/if}}
            </a>
            {{/each}}
          </div>
</div>

               </div>      
                   
             
          
     
       </div>
       </div>
          
<div class="col-sm-4 mb-3"></div>
